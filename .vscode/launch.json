{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug QuantStrategyClient (WSL)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/quant_enterprise_strategy/src/bin/QuantStrategyClient",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/quant_enterprise_strategy/src/bin",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}

