{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 22,
    "patch": 1
  },
  "configurePresets": [
    {
      "name": "linux-debug",
      "displayName": "Linux Debug (WSL)",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/quant_enterprise_strategy/build",
      "sourceDir": "${sourceDir}/quant_enterprise_strategy/src",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "linux-release",
      "displayName": "Linux Release (WSL)",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/quant_enterprise_strategy/build",
      "sourceDir": "${sourceDir}/quant_enterprise_strategy/src",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "build-debug",
      "configurePreset": "linux-debug",
      "jobs": 0
    },
    {
      "name": "build-release",
      "configurePreset": "linux-release",
      "jobs": 0
    }
  ]
}

